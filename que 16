public class programshomework
{
    public static int sumif()
    {
        int sum = 0;
        int i;
        int[] array = new int[]{1,6,4,7,9};
        for (i= 0;i<array.length;i++)
        {
            if (array[i]==6)
            {
                for (int j = i;j<array.length;j++)
                {
                    if (array[j]==7)
                    {
                        sum = i +j;
                    }
                }
            }
        }
        return -1;
    }
}
