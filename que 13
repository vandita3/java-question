public class programshomework
 {
      static class box
     {

     int width;

     int height;

    int depth;

    public box(int width, int height, int depth) {

        this.width = width;

        this.height = height;

        this.depth = depth;

    }

    public  int Volume() {

        return width* height* depth;

    }


}

    public static void main(String[] args) {

        box b1 = new box(16,9,4);

        System.out.println(b1.Volume());

    }

}
